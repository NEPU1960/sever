<script type="text/javascript">
	function styleTable(tableId) {
		var tab = document.getElementById(tableId);
		if (tab) {
			var tbody = tab.getElementsByTagName("tbody")[0];
			var trArray = tbody.childNodes;
			
			var unStyleIndex = 3;
			
			for (var i = 0; i < unStyleIndex; i++) {
				var firstTr = trArray[i];
				var firstTrTdArray = firstTr.childNodes;
				
				for (var j = 0, jlen = firstTrTdArray.length; j < jlen; j++) {
					firstTrTdArray[j].style.border = "0px none black";
				}
			}
			
			for (var i = unStyleIndex, ilen = trArray.length; i < ilen; i++) {
				var firstTr = trArray[i];
				var firstTrTdArray = firstTr.childNodes;
				
				for (var j = 0, jlen = firstTrTdArray.length; j < jlen; j++) {
					firstTrTdArray[j].style.border = "1px solid black";
					firstTrTdArray[j].align = "center";
				}
			}
		}
	}
	
	styleTable("xjkpTable");
	
