	for(i=0;i<len;i++){
		if(kcsj.indexOf(str.substr(i*2,2))==0||kcsj.indexOf(str.substr(i*2,2))==2||kcsj.indexOf(str.substr(i*2,2))==4){
			return true;
		}
	}
	return false;
}
